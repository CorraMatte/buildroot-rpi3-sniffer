#!/bin/sh

export GPS_DIR=/root/scripts/gps
export CAN_DIR=/root/scripts/can
export CAMERA_DIR=/root/scripts/camera

# Initialize interface #
export UART_INTERFACE=/dev/ttyAMA0

# Initialize python library # 
export PYTHONPATH=/root/pythonlib/

# Insert pcan module #
insmod /lib/modules/"$(uname -r)"/kernel/drivers/peak/pcan.ko
